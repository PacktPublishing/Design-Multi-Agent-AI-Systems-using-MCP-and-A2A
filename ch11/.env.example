# K8s-ai API Key
# Generate a real key using: k8s-ai-server --generate-key --client-name "MAKDO"
# For development, you can use: test-key
K8S_AI_API_KEY=your-api-key-here

# OpenAI API Key (required for k8s-ai LLM calls)
OPENAI_API_KEY=your-openai-api-key-here

# Slack credentials (required for MAKDO Slack Bot)
AI6_BOT_TOKEN=xoxb-your-slack-bot-token
AI6_APP_TOKEN=xapp-your-slack-app-token
SLACK_TEAM_ID=your-slack-team-id
