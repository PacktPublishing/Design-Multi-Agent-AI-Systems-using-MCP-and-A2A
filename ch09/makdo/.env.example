# MAKDO Environment Variables Example
# Copy to .env and configure with your actual values

# OpenAI API Key (for AI-6 agents)
OPENAI_API_KEY=your_openai_api_key_here

# Slack Configuration (from /Users/gigi/git/ai-six/py/frontend/slack/.env)
AI6_BOT_TOKEN=
AI6_APP_TOKEN=
AI6_CLIENT_SECRET=
AI6_SIGNING_SECRET=

# Slack Team/Workspace ID (required for MCP server)
SLACK_TEAM_ID=

# k8s-ai Server Configuration
K8S_AI_BASE_URL=http://localhost:9999
K8S_AI_ADMIN_URL=http://localhost:9999/admin

# Optional: Override default model
DEFAULT_MODEL_ID=gpt-4o

# Optional: Custom configuration file path
MAKDO_CONFIG_PATH=config/makdo.yaml
